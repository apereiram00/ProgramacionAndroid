<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/avionesFragment">

    <fragment
        android:id="@+id/avionesFragment"
        android:name="com.example.skyrivals.AvionesFragment"
        android:label="fragment_aviones"
        tools:layout="@layout/fragment_aviones" >
        <action
            android:id="@+id/action_avionesFragment_to_detallesFragment"
            app:destination="@id/detallesFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/favoritosFragment"
        android:name="com.example.skyrivals.FavoritosFragment"
        android:label="fragment_favoritos"
        tools:layout="@layout/fragment_favoritos" >
        <action
            android:id="@+id/action_favoritosFragment_to_detallesFragment"
            app:destination="@id/detallesFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/combateFragment"
        android:name="com.example.skyrivals.CombateFragment"
        android:label="fragment_combate"
        tools:layout="@layout/fragment_combate" />
    <fragment
        android:id="@+id/luftwaffeFragment"
        android:name="com.example.skyrivals.LuftwaffeFragment"
        android:label="fragment_luftwaffe"
        tools:layout="@layout/fragment_luftwaffe" />
    <fragment
        android:id="@+id/usaafFragment"
        android:name="com.example.skyrivals.UsaafFragment"
        android:label="fragment_usaaf"
        tools:layout="@layout/fragment_usaaf" />
    <fragment
        android:id="@+id/rafFragment"
        android:name="com.example.skyrivals.RafFragment"
        android:label="fragment_raf"
        tools:layout="@layout/fragment_raf" />
    <fragment
        android:id="@+id/ussrFragment"
        android:name="com.example.skyrivals.UssrFragment"
        android:label="fragment_ussr"
        tools:layout="@layout/fragment_ussr" />
    <fragment
        android:id="@+id/detallesFragment"
        android:name="com.example.skyrivals.DetallesFragment"
        android:label="fragment_detalles"
        tools:layout="@layout/fragment_detalles" />
    <fragment
        android:id="@+id/avionesListFragment"
        android:name="com.example.skyrivals.AvionesListFragment"
        android:label="fragment_aviones_list"
        tools:layout="@layout/fragment_aviones_list" />


</navigation>
